    "@typescript-eslint/parser": "^6.9.1",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "eslint": "^8.53.0",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
